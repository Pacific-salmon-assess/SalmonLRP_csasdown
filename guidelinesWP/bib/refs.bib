@article{portleyLimitReferencePoints2014,
  title = {Limit {{Reference Points}} for {{Pacific Salmon Fisheries}}},
  author = {Portley, Nicole and Geiger, Harold J.},
  year = {2014},
  month = mar,
  journal = {North American Journal of Fisheries Management},
  volume = {34},
  number = {2},
  pages = {401--410},
  issn = {0275-5947, 1548-8675},
  doi = {10.1080/02755947.2014.882453},
  abstract = {Limit reference points'' define a state at which fishery management has reached overfishing, overfished stock status, or some other regulatory or conservation point of concern. In many Pacific salmon Oncorhynchus spp. fishery jurisdictions, management only uses ``target reference points''\textemdash the specific numerical management objective intended to bring about some fishery benefit. There are several reasons to adopt limit reference points, and jurisdictions without previously defined limit reference points could adopt them in order to align better with the Magnuson\textendash Stevens Act (American fisheries) or, more importantly, to facilitate cross-jurisdiction sustainability assessments, which are increasingly needed for certifications of sustainability. In actual practice, the stock size of 0.5 times a target reference point has been used as a limit reference point for some Pacific salmon fisheries, or at least as a proxy for a limit reference point. We term this point the ``minimum stock size threshold,'' following a convention already in use, and we note that limit reference points defined this way are increasingly being accepted as a standard either because of justification based on simulation and fishery principles or for purely pragmatic reasons.},
  langid = {english},
  file = {C\:\\Users\\HoltC\\Zotero\\storage\\F69BSGJP\\Portley and Geiger - 2014 - Limit Reference Points for Pacific Salmon Fisherie.pdf}
}

@incollection{puntHarvestControlRules2010,
  title = {Harvest {{Control Rules}} and {{Fisheries Management}}},
  booktitle = {Handbook of {{Fisheries Conservation}} and {{Management}}},
