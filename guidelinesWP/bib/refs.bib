	file = {Peterman et al. - 1998 - Patterns of covariation in survival rates of Briti.pdf:C\:\\Users\\holtk\\Zotero\\storage\\97A6QMJ8\\Peterman et al. - 1998 - Patterns of covariation in survival rates of Briti.pdf:application/pdf},
