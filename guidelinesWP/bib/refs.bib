
@article{adkisonReviewSalmonSpawnerRecruitment2021,
  title = {A {{Review}} of {{Salmon Spawner}}-{{Recruitment Analysis}}: The {{Central Role}} of the {{Data}} and {{Its Impact}} on {{Management Strategy}}},
