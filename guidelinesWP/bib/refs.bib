	pages = {2208--2220},
	file = {Punt et al. - 2014 - Fisheries management under climate and environment.pdf:C\:\\Users\\holtk\\Zotero\\storage\\EU42PG5C\\Punt et al. - 2014 - Fisheries management under climate and environment.pdf:application/pdf},
}

@article{olearyComparisonMultipleApproaches2020,
	title = {Comparison of multiple approaches to calculate time-varying biological reference points in climate-linked population-dynamics models},
	volume = {77},
	issn = {1054-3139, 1095-9289},
	url = {https://academic.oup.com/icesjms/article/77/3/930/5637330},
	doi = {10.1093/icesjms/fsz215},
	abstract = {Fisheries managers use biological reference points (BRPs) as targets or limits on ﬁshing and biomass to maintain productive levels of ﬁsh stock biomass. There are multiple ways to calculate BRPs when biological parameters are time varying. Using summer ﬂounder (Paralichthys dentatus) as a case study, we investigated time-varying approaches in concert with climate-linked population models to understand the impact of environmentally driven variability in natural mortality, recruitment, and size-at-age on two commonly used BRPs ½B0ðtÞ and F35\%(t)]. We used the following two approaches to calculate time-varying BRPs: dynamic-BRP and moving-average-BRP. We quantiﬁed the variability and uncertainty of different climate dependencies and estimation approaches, attributed BRP variation to variation in life-history processes, and evaluated how using different approaches impacts estimates of stock status. Results indicate that the dynamic-BRP approach using the climate-linked natural mortality model produced the least variable reference points compared to others calculated. Summer ﬂounder stock status depended on the estimation approach and climate model used. These results emphasize that understanding climate dependencies is important for summer ﬂounder reference points and perhaps other species, and careful consideration is warranted when considering what time-varying approach to use, ideally based upon simulation studies within a proposed set of management procedures.},
	language = {en},
	number = {3},
	urldate = {2021-11-10},
	journal = {ICES J. Mar. Sci.},
	author = {O’Leary, Cecilia A and Thorson, James T and Miller, Timothy J and Nye, Janet A},
