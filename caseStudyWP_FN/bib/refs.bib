	file = {Kell et al. - 2021 - Validation of stock assessment methods is it me o.pdf:C\:\\Users\\WarkentinL\\Zotero\\storage\\WYJVSFP6\\Kell et al. - 2021 - Validation of stock assessment methods is it me o.pdf:application/pdf},
