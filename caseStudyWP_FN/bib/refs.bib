	file = {Litz et al. - 2021 - Competition with odd-year pink salmon in the ocean.pdf:C\:\\Users\\WarkentinL\\Zotero\\storage\\VFBDMEXR\\Litz et al. - 2021 - Competition with odd-year pink salmon in the ocean.pdf:application/pdf},
