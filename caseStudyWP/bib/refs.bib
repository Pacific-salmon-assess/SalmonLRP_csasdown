@article{litzCompetitionOddyearPink2021a,
  title = {Competition with Odd-Year Pink Salmon in the Ocean Affects Natural Populations of Chum Salmon from {{Washington}}},
  author = {Litz, Mnc and Agha, M and Dufault, Am and Claiborne, Am and Losee, Jp and Anderson, Aj},
