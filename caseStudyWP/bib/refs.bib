  file = {C\:\\Users\\HoltC\\Zotero\\storage\\YGHGBLD2\\Ohlberger et al. - 2019 - A Bayesian life-cycle model to estimate escapement.pdf}
}

@article{olmosEvidenceSpatialCoherence2019,
